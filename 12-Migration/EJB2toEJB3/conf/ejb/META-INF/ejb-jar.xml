<ejb-jar
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	                    http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	version="3.0">
	<description>EJB2toEJB3</description>
	<display-name>EJB2toEJB3</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>EJB2Bean</ejb-name>
			<home>ch.itenengineering.ejb2toejb3.ejb.EJB2Home</home>
			<remote>ch.itenengineering.ejb2toejb3.ejb.EJB2Remote</remote>
			<ejb-class>ch.itenengineering.ejb2toejb3.ejb.EJB2Bean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
</ejb-jar>
 
